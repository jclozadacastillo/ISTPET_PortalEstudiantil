@{
    Layout = null;
}

<!DOCTYPE html>

<html>

<head>
    <meta charset="utf-8">
    <title>INSTITUTO TRAVERSARI: Inscripciones en línea</title>
    <meta content="width=device-width, initial-scale=1.0" name="viewport">
    <link href="~/templateInscripciones/img/icono.png" rel="icon">
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link
        href="https://fonts.googleapis.com/css2?family=Jost:wght@500;600;700&family=Open+Sans:wght@400;500&display=swap"
        rel="stylesheet">
    <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.0/css/all.min.css" rel="stylesheet">
    <link href="~/utilities/bootstrap-icons-1.8.1/bootstrap-icons.css" rel="stylesheet" />
    <link href="~/templateInscripciones/lib/animate/animate.min.css" rel="stylesheet">
    <link href="~/templateInscripciones/lib/owlcarousel/assets/owl.carousel.min.css" rel="stylesheet">
    <link href="~/templateInscripciones/css/bootstrap.min.css" rel="stylesheet">
    <link href="~/templateInscripciones/css/style.css" rel="stylesheet">
    <link href="~/templateInscripciones/css/custom-scrollbar.css" rel="stylesheet">
    <link href="~/utilities/toast/style.css" rel="stylesheet" />
    <script defer src="~/utilities/axios/axios.min.js"></script>
    <script defer src="~/utilities/jquery-3.6.0.min.js"></script>
    <script defer src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0/dist/js/bootstrap.bundle.min.js"></script>
    <script defer src="~/utilities/moment-with-locales.js"></script>
    <script defer src="~/templateInscripciones/lib/wow/wow.min.js"></script>
    <script defer src="~/templateInscripciones/lib/easing/easing.min.js"></script>
    <script defer src="~/templateInscripciones/lib/waypoints/waypoints.min.js"></script>
    <script defer src="~/templateInscripciones/lib/owlcarousel/owl.carousel.min.js"></script>
    <script defer src="~/templateInscripciones/lib/counterup/counterup.min.js"></script>
</head>

<body>
    <div class="container-fluid bg-primary" style="min-height: 100vh;">
        <div class="row">

            <div class="col-sm-6 m-3 mx-auto" style="padding-top: 15vh;">
                <div class="card">
                    <div class="card-body">
                        <div class="col-sm-12 m-2 text-center">
                            <img src="~/templateInscripciones/img/logo.png" width="271">
                        </div>
                        <div class='@(ViewData["clase"])'
                            style="height: 37vh;width: 100vw;display:table-cell;vertical-align:middle">
                            @Html.Raw(ViewData["mensaje"])
                        </div>
                        <div class="text-center mt-3">
                            <a class='btn btn-primary' href="https://institutotraversari.edu.ec/">Entendido</a>
                        </div>
                    </div>
                </div>
            </div>
        </div>

    </div>
</body>

</html>
